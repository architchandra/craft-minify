{
    "name": "nystudio107/craft3-minify",
    "description": "A simple plugin that allows you to minify blocks of HTML, CSS, and JS inline in Craft CMS templates.",
    "type": "craft-plugin",
    "version": "1.2.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "minify"
    ],
    "support": {
        "docs": "https://github.com/nystudio107/craft3-minify/blob/master/README.md",
        "issues": "https://github.com/nystudio107/craft3-minify/issues"
    },
    "license": "MIT",
    "authors": [
        {
          "name": "nystudio107",
          "homepage": "https://nystudio107.com/"
        }
    ],
    "require": {
        "mrclay/minify": "^2.3.0"
    },
    "autoload": {
        "psr-4": {
          "nystudio107\\minify\\": "src/"
        }
    },
    "extra": {
        "name": "Minify",
        "handle": "minify",
        "schemaVersion": "1.0.0",
        "hasSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/nystudio107/craft3-minify/CHANGELOG.md",
        "class": "nystudio107\\minify\\Minify",
        "components": {
            "minify": "nystudio107\\minify\\services\\MinifyService"
        }
    }
}
